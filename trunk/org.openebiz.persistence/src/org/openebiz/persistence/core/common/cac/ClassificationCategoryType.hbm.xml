<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field" package= "org.openebiz.core.common.cac">
    <class name="ClassificationCategoryType" dynamic-update="true" dynamic-insert="true" table="classification_category_type">
        <id name="internalId" type="long" unsaved-value="null" column="internal_id">
            <generator class="native" />
        </id>
        <version name="version" unsaved-value="null" column="version" />
        <many-to-one name="description" entity-name="org.openebiz.core.common.cbc.DescriptionType" cascade="save-update" not-null="false"
        column="description_type_internal_id" />
        <component name="name" update="true" insert="true" class="org.openebiz.core.common.cbc.NameType">
            <property name="value" type="string" column="name_value" />
            <property name="languageID" type="string" column="name_languageid" />
        </component>
        <component name="codeValue" update="true" insert="true" class="org.openebiz.core.common.cbc.CodeValueType">
            <property name="value" type="string" column="code_value_value" />
            <property name="languageID" type="string" column="code_value_languageid" />
        </component>
        <list access="field" name="categorizesClassificationCategory" cascade="save-update">
            <key column="categorizes_classification_category_fk" />
            <list-index column="categorizes_classification_category_idx" />
            <one-to-many entity-name="org.openebiz.core.common.cac.ClassificationCategoryType" />
        </list>
    </class>
</hibernate-mapping>
